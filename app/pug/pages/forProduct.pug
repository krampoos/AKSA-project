	body
		include ../navBar.pug
		sectiom.forProd
			.img
		section.APD12
			div.secContainer
				h3.apd__title APD 12 A
				.seperator
				.product
					h3.product__title ПРОДУКТЫ
					div.product__info
						p.product__paraph 
							| ПРОДУКТЫ : 
							span AKSA
						p.product__paraph 
							| ГЕНЕРАТОР: 
							span AK 109 
						p.product__paraph 
							| МОДЕЛЬ ПОНЕЛИ УПРАВЛЕНИЯ: 
							span DSE 6020
					.accordion
							dl
								dt
									a.accordion-title.accordionTitle.js-accordionTrigger(href="#accordion1" aria-expanded="false" aria-controls="accordion1") О продукте
								dd#accordion1.accordion-content.accordionItem.is-collapsed(aria-hidden="true")
									p
										|Aksa power generation system, providing optimum performance, and reliability, for stationary standby, prime power, and continuous duty applications. All generator sets are factory build, and production tested.
								dt
									a.accordion-title.accordionTitle.js-accordionTrigger(href="#accordion2" aria-expanded="false" aria-controls="accordion2")
										| СЕРТИФИКАТЫ
								dd#accordion2.accordion-content.accordionItem.is-collapsed(aria-hidden="true")
									p TS ISO 8528
									p CE
									p SZUTEST
									p 2000/14/EC
								dt
									a.accordion-title.accordionTitle.js-accordionTrigger(href="#accordion3" aria-expanded="false" aria-controls="accordion3")
										| Общие Характеристики
								dd#accordion3.accordion-content.accordionItem.is-collapsed(aria-hidden="true")
									table.table
										tr.table-column
											td.column-item Название Модели
											td.column-item APD 12 A
										tr.table-column
											td.column-item Частота (Гц)
											td.column-item 50
										tr.table-column
											td.column-item вид используемого топлива
											td.column-item Diesel
										tr.table-column
											td.column-item бренд и модель двигателя
											td.column-item AKSA - A3CRX14
										tr.table-column
											td.column-item генератор переменного тока марки и модели
											td.column-item Aksa - AK 109 
										tr.table-column
											td.column-item Модель панели управления
											td.column-item DSE 6020 
										tr.table-column 
											td.column-item кожуха
											td.column-item ANS 1

		include ../footer.pug

